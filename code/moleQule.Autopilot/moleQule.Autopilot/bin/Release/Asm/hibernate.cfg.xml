<?xml version='1.0' encoding='utf-8'?>
<hibernate-configuration xmlns="urn:nhibernate-configuration-2.0">

  <!-- an ISessionFactory instance -->
  <session-factory>

    <!-- properties -->
    <property name="connection.provider">NHibernate.Connection.DriverConnectionProvider</property>
    <property name="connection.driver_class">NHibernate.Driver.NpgsqlDriver</property>
    <property name="connection.connection_string">Server=servidor.aerotrainingcanarias.com;User Id=moladmin;Password=******;Database=HANGAR;Encoding=UNICODE;</property>
    <property name="default_schema">COMMON</property>
    <property name="show_sql">false</property>
    <property name="dialect">NHibernate.Dialect.PostgreSQLDialect</property>
    <property name="use_outer_join">true</property>
    <property name="use_direct_sql">true</property>
    <property name="default_user"></property>

    <!-- moleQule -->

    <mapping resource="moleQule.Library.Asm.Common.ItemMap.hbm.xml" assembly="moleQule.Library" />
    <mapping resource="moleQule.Library.Asm.Common.SecureItem.hbm.xml" assembly="moleQule.Library" />
    <mapping resource="moleQule.Library.Asm.Common.SchemaUser.hbm.xml" assembly="moleQule.Library" />
    <mapping resource="moleQule.Library.Asm.Common.User.hbm.xml" assembly="moleQule.Library" />
    <mapping resource="moleQule.Library.Asm.Common.UserSetting.hbm.xml" assembly="moleQule.Library" />
    <mapping resource="moleQule.Library.Asm.Common.Variable.hbm.xml" assembly="moleQule.Library" />

    <mapping resource="moleQule.Library.Asm.nh0001.ItemLicences.hbm.xml" assembly="moleQule.Library" />
    <mapping resource="moleQule.Library.Asm.nh0001.Setting.hbm.xml" assembly="moleQule.Library" />

    <!-- moleQule.Common -->

    <mapping resource="moleQule.Library.Common.Asm.Common.Cargo.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.Common.ContactoEmpresa.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.Common.Empresa.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.Common.Municipio.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.Common.Schema.hbm.xml" assembly="moleQule.Library.Common" />

    <mapping resource="moleQule.Library.Common.Asm.nh0001.CuentaBancaria.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.nh0001.Impuesto.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.nh0001.LineaRegistro.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.nh0001.Registro.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.nh0001.TarjetaCredito.hbm.xml" assembly="moleQule.Library.Common" />
    <mapping resource="moleQule.Library.Common.Asm.nh0001.TPV.hbm.xml" assembly="moleQule.Library.Common" />

    <!-- moleQule.Hipatia -->

    <mapping resource="moleQule.Library.Hipatia.Asm.Common.Entidad.hbm.xml" assembly="moleQule.Library.Hipatia" />
    <mapping resource="moleQule.Library.Hipatia.Asm.Common.TipoEntidad.hbm.xml" assembly="moleQule.Library.Hipatia" />
    <mapping resource="moleQule.Library.Hipatia.Asm.Common.Tipodocumento.hbm.xml" assembly="moleQule.Library.Hipatia" />

    <mapping resource="moleQule.Library.Hipatia.Asm.nh0001.Agente.hbm.xml" assembly="moleQule.Library.Hipatia" />
    <mapping resource="moleQule.Library.Hipatia.Asm.nh0001.Documento.hbm.xml" assembly="moleQule.Library.Hipatia" />
    <mapping resource="moleQule.Library.Hipatia.Asm.nh0001.AgenteDocumento.hbm.xml" assembly="moleQule.Library.Hipatia" />

    <!-- Hangar -->

  </session-factory>

</hibernate-configuration>